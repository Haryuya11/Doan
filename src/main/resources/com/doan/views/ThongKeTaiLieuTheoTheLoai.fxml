<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.doan.controllers.ThongKeTaiLieuTheoTheLoaiController">
    <padding><Insets top="20" right="20" bottom="20" left="20"/></padding>

    <Label text="Thống Kê Tài Liệu Theo Thể Loại" style="-fx-font-size: 24;"/>

    <TableView fx:id="tableViewKetQua">
        <columns>
            <TableColumn text="Thể Loại" fx:id="colTenLoai"/>
            <TableColumn text="Số Lượng" fx:id="colSoLuong"/>
        </columns>
    </TableView>
</VBox>
